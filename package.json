{
  "name": "arssh2-client",
  "version": "1.0.4",
  "description": "ssh2 client wrapper with auto-recovery functionality",
  "author": {
    "name": "Naoyuki Sogo"
  },
  "license": "MIT",
  "repository": "github:so5/arssh2-client",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier-eslint --write */*.js",
    "mocha": "mocha",
    "mocha-v": "cross-env DEBUG=arssh2:* mocha",
    "mocha-vv": "cross-env DEBUG=arssh2:*,sbs:* mocha",
    "mocha-vvv": "cross-env DEBUG=arssh2:*,ssh2:*,sbs:* mocha",
    "mocha-vvvv": "cross-env DEBUG=* mocha",
    "check": "run-s format lint test",
    "lint": "eslint lib/*.js test/*.js",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "chai-things": "^0.2.0",
    "debug": "^3.1.0",
    "fs-extra": "^5.0.0",
    "minimatch": "^3.0.4",
    "promise-retry": "^1.1.1",
    "simple-batch-system": "^1.4.7",
    "ssh2": "^0.5.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-node": "^5.2.1",
    "jsdoc-to-markdown": "^3.1.0-0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.0",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0"
  }
}
